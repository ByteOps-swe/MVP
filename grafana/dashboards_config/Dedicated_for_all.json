matico'\r\n                AND (\r\n                    (toUnixTimestamp($__toTime) - toUnixTimestamp($__fromTime)) > 2592000\r\n                )\r\n            )\r\n        ) THEN toStartOfMonth(timestamp)\r\n        ELSE timestamp\r\n    END AS timestamp\r\nFROM innovacity.$tabella\r\nWHERE\r\n cella IN ($Cella) \r\n    AND $__timeFilter(timestamp)\r\n    AND ID_sensore IN ($sensors_id)\r\nGROUP BY ID_sensore,timestamp\r\nHAVING\r\n  value >= ($Min)\r\n    AND value <= ($Max)\r\n",
